        "rxjs": "^6.5.3"
    },
    "devDependencies": {
        "@types/jest": "^24.0.24",
        "coveralls": "^3.0.9",
        "jest": "^24.9.0",
        "prettier": "^1.19.1",
        "ts-jest": "^24.2.0",
        "typescript": "^3.7.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
