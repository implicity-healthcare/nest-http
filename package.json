{
    "name": "@implicity/nest-http",
    "version": "1.0.0",
    "description": "description",
    "author": "",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
