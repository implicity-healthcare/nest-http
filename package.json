        "rxjs": "6.5.2"
    },
    "devDependencies": {
        "@types/jest": "24.0.14",
        "coveralls": "3.0.4",
        "jest": "24.8.0",
        "prettier": "1.18.2",
        "ts-jest": "24.0.2",
        "typescript": "3.5.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
